{
  "expo": {
    "name": "Handwriting Orca Math",
    "slug": "handwriting-orca-math",
    "scheme": "orcamath",
    "version": "1.0.0",
    "orientation": "portrait",
    "sdkVersion": "52.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "plugins": [
      "expo-dev-client"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.doinfinitely.handwritingorcamath",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true,
          "NSAllowsLocalNetworking": true
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "extra": {
      "expoPublic": {
        "MATHPIX_PROXY_URL": "http://10.10.0.202:5056/recognize",
        "LLM_API_URL": "http://10.10.0.202:5056/validate"
      },
      "eas": {
        "projectId": "de513976-d38c-42fe-aed0-84f6591a2d3f"
      }
    }
  }
}
